local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local X = Material.Load({
    Title = "Keybrew Hub | Mining Clicker Sim",
    Style = 3,
    SizeX = 300,
    SizeY = 350,
    Theme = "Light",
    ColorOverrides = {
        MainFrame = Color3.fromRGB(0,0,0),
        Toggle = Color3.fromRGB(124,37,255),
        ToggleAccent = Color3.fromRGB(255,255,255), 
        Dropdown = Color3.fromRGB(124,37,255),
		DropdownAccent = Color3.fromRGB(255,255,255),
        Slider = Color3.fromRGB(124,37,255),
		SliderAccent = Color3.fromRGB(255,255,255),
        NavBarAccent = Color3.fromRGB(0,0,0),
        Content = Color3.fromRGB(0,0,0),
    }
})

local Y = X.New({
    Title = "Main"
})

local Z = X.New({
    Title = "Misc"
})

local Cred = X.New({
    Title = "Credits"
})
    Cred.Button({
    Text = "Copy Discord UyuSF4fPC9",
    Callback = function()
        setclipboard("https://discord.gg/UyuSF4fPC9")
        toclipboard("https://discord.gg/UyuSF4fPC9")
    end,
})

Y.Toggle({
    Text = "Autoclick",
    Callback = function(Value)
        a = Value
        while a do task.wait()
            game:GetService("ReplicatedStorage").Remotes.Click:InvokeServer()
            game:GetService("ReplicatedStorage").Remotes.Bonus:InvokeServer()

        end
    end,
    Enabled = false
})

Y.Dropdown({
    Text = "Egg Name",
    Callback = function(Value)
        getgenv().egg = Value    
    end,
    Options = {
        "Starter Egg",
        "Spotted Egg",
        "Floral Egg",
        "Desert Egg",
        "Snow Egg",
        "Cave Egg",
        "Ocean Egg",
        "Jungle Egg",
        "Volcano Egg",
        "Space Egg",
        "Undead Egg",
        "Heavenly Egg",
    },
})

Y.Dropdown({
    Text = "How many",
    Callback = function(Value)
        getgenv().num = Value    
    end,
    Options = {
        "1",
        "3"
    },
})

Y.Toggle({
    Text = "Auto Open Egg",
    Callback = function(Value)
        aa = Value
        while aa do task.wait()
            local args = {
                [1] = egg,
                [2] = tonumber(num)
            }

            game:GetService("ReplicatedStorage").Remotes.buyEgg:InvokeServer(unpack(args))
        end
    end,
    Enabled = false
})

Y.Toggle({
    Text = "Auto Buy Pickaxes",
    Callback = function(Value)
        aaa = Value
        while aaa do task.wait()
                    for i, v in pairs(game:GetService("ReplicatedStorage").Pickaxes:GetChildren()) do
                        if v:IsA "Tool"  then
                    
                    local args = {
                        [1] = v.Name
                    }
                    
                    game:GetService("ReplicatedStorage").Remotes.BuyPickaxe:InvokeServer(unpack(args))
                    wait(0.5)
                end
            end
        end
    end,
    Enabled = false
})

Z.Toggle({
    Text = "Auto Claim Achievements",
    Callback = function(Value)
        a = Value
        while a do task.wait()
            local args = {
                [1] = "hatchEgg"
            }

            game:GetService("ReplicatedStorage").Remotes.claimAchievment:FireServer(unpack(args))

            local args = {
                [1] = "mineOre"
            }
            
            game:GetService("ReplicatedStorage").Remotes.claimAchievment:FireServer(unpack(args))        
            local args = {
                [1] = "unlockArea"
            }

            game:GetService("ReplicatedStorage").Remotes.claimAchievment:FireServer(unpack(args))

        end
    end,
    Enabled = false
})

Z.Toggle({
    Text = "Auto Rebirth",
    Callback = function(Value)
        aaaa = Value
        while aaaa do task.wait()
            game:GetService("ReplicatedStorage").Remotes.Rebirth:FireServer()
        end
    end,
    Enabled = false
})

Z.Slider({
    Text = "WalkSpeed",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value        
    end,
    Min = 20,
    Max = 500,
    Def = 20
})
